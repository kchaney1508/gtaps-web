---
layout: null
permalink: /sitemap.xml
sitemap: false
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% assign crawlable_pages = site.pages | where_exp: "item", "item.sitemap != false" %}
  {% for item in crawlable_pages %}
  <url>
    <loc>{{ item.url | absolute_url }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
  </url>
  {% endfor %}
  {% for item in site.changelog %}
  <url>
    <loc>{{ item.url | absolute_url }}</loc>
    <lastmod>{{ item.published_on | default: site.time | date_to_xmlschema }}</lastmod>
  </url>
  {% endfor %}
  {% for item in site.information %}
  <url>
    <loc>{{ item.url | absolute_url }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
  </url>
  {% endfor %}
</urlset>
